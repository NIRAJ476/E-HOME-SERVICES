<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.e_homeservices"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.SEND_SMS" />

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="example.e_homeservices.Login"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="example.e_homeservices.SignUp"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="example.e_homeservices.Forgot_password"
            android:label="@string/title_activity_forgot_password" >
        </activity>
        <activity
            android:name="example.e_homeservices.Home"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="example.e_homeservices.Services"
            android:label="@string/title_activity_services" >
        </activity>
        <activity
            android:name="example.e_homeservices.Painter"
            android:label="@string/title_activity_painter" >
        </activity>
        <activity
            android:name="example.e_homeservices.Painting1"
            android:label="@string/title_activity_painting1" >
        </activity>
        <activity
            android:name="example.e_homeservices.Electrical"
            android:label="@string/title_activity_electrical" >
        </activity>
        <activity
            android:name="example.e_homeservices.Electrical1"
            android:label="@string/title_activity_electrical1" >
        </activity>
        <activity
            android:name="example.e_homeservices.Electrical2"
            android:label="@string/title_activity_electrical2" >
        </activity>
        <activity
            android:name="example.e_homeservices.Carpenter"
            android:label="@string/title_activity_carpenter" >
        </activity>
        <activity
            android:name="example.e_homeservices.Carpenter1"
            android:label="@string/title_activity_carpenter1" >
        </activity>
        <activity
            android:name="example.e_homeservices.Carpenter2"
            android:label="@string/title_activity_carpenter2" >
        </activity>
        <activity android:name="example.e_homeservices.Activity_Provider" >
        </activity>
        <activity android:name="example.e_homeservices.HomeProvider" >
        </activity>
        <activity android:name="example.e_homeservices.SendService" >
        </activity>
        <activity
            android:name="example.e_homeservices.Splash"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/FullscreenTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="example.e_homeservices.Plumber"
            android:label="@string/title_activity_plumber" >
        </activity>
        <activity
            android:name="example.e_homeservices.Plumber1"
            android:label="@string/title_activity_plumber1" >
        </activity>
        <activity
            android:name="example.e_homeservices.Thanks"
            android:label="@string/title_activity_thanks" >
        </activity>
    </application>

</manifest>
